menuconfig RPR_MODULE_SWITCH
    bool "Enable Switch module"
    select GPIO
	  default y
    help
      Enable support for switch reading.
      Automatically selects CONFIG_GPIO.

if RPR_MODULE_SWITCH
config RPR_MODULE_SWITCH_LOG_LEVEL
    int "Switch module log level"
    range 0 4
    default 3
    help
      0=OFF, 1=ERROR, 2=WARNING, 3=INFO, 4=DEBUG
endif