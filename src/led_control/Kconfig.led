menuconfig RPR_MODULE_LED
    bool "Enable LED control module"
    select GPIO
	  default y
    help
      Enable support for controlling LEDs.
      Automatically selects CONFIG_GPIO.

if RPR_MODULE_LED
config RPR_MODULE_LED_LOG_LEVEL
    int "LED module log level"
    range 0 4
    default 3
    help
      0=OFF, 1=ERROR, 2=WARNING, 3=INFO, 4=DEBUG
endif